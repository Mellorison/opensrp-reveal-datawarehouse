-- Verify reveal_raw_tables:raw_tasks on pg

BEGIN;

SET search_path TO :"schema",public;

SELECT id, server_version, data
FROM raw_tasks
WHERE FALSE;

-- check primary key
SELECT 1/COUNT(*)
FROM pg_catalog.pg_constraint
WHERE
conname = 'raw_tasks_pkey'
AND contype = 'p'
AND 1 = ALL(conkey);

ROLLBACK;
