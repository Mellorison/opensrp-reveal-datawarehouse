-- Verify reveal_raw_tables:raw_organizations on pg

BEGIN;

SET search_path TO :"schema",public;

SELECT id, data, date_created, last_updated, warehouse_id
FROM raw_organizations
WHERE FALSE;

-- check primary key
SELECT 1/COUNT(*)
FROM pg_catalog.pg_constraint
WHERE
conname = 'raw_organizations_pkey'
AND contype = 'p'
AND 1 = ALL(conkey);


ROLLBACK;
