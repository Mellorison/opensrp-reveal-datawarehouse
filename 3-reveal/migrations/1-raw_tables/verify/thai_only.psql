-- Verify reveal_raw_tables:thai_only on pg

BEGIN;

SET search_path TO :"schema",public;

SELECT id, server_version, data
FROM raw_structures
WHERE FALSE;

-- check primary key
SELECT 1/COUNT(*)
FROM pg_catalog.pg_constraint
WHERE
conname = 'raw_structures_pkey'
AND contype = 'p'
AND 1 = ALL(conkey);

ROLLBACK;
